<h1>New Crop Definition</h1>

<% form_for(@crop_definition) do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    
    <div class="bw">
      <%= f.submit 'Create Crop Definition' %> - or - <%= link_to 'Cancel', crop_definitions_path %>
    </div>
  </fieldset>
<% end %>