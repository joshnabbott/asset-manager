<h1>New Asset Category</h1>

<% form_for(@asset_category) do |f| %>
  <fieldset>
    <%= f.error_messages %>
    <div class="fw">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="bw">
      <%= f.submit 'Create Asset Category' %> - or - <%= link_to 'Cancel', asset_categories_path %>
    </div>
  </fieldset>
<% end %>